    "angular2-google-maps": "^0.16.0",
    "body-parser": "^1.17.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "express": "^4.15.3",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
